<!--To display the todos-->
<% for(var i =0; i< allTodos.length ; i++){%>
            <tr>
                <td>
                    <%= 
                    allTodos[i].completed ? '[x]':'[ ]'
                    %>
                </td>
                <td>
                    <%= allTodos[i].title %>
                </td>
                <td>
                    <%= allTodos[i].dueDate %>
                </td>
            </tr>
        <% } %>